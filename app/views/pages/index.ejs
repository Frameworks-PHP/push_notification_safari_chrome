<!DOCTYPE html>
<html>
    <head>
        <% include ../partials/head %>
    </head>
    <body>
        <% include ../partials/header %>

        <div class="page-content">
            <div class="row">
                <% include ../partials/navigation %>
                <div class="col-md-10">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="content-box-large">
                                <div class="panel-heading">
                                    <div class="panel-title">Push Notification</div>
                                </div>
                                <div class="panel-body">
                                    <form action="" id="formPushNotification">
                                        <fieldset class="col-sm-6">
                                            <div class="form-group">
                                                <label>Select Domain</label>
                                                <select class="form-control" id="list-domain-send-noti" name="domain">
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <label>Web Push ID</label>
                                                <input type="text" name="webpushid" class="form-control" id="list-webpushid-send-noti" readonly>
                                            </div>
                                            <div class="form-group">
                                                <label>Notification title</label>
                                                <input type="text" name="title" class="form-control" id="notification-title" placeholder="Insert here a title">
                                            </div>
                                        </fieldset>
                                        <fieldset class="col-sm-6">
                                            <div class="form-group">
                                                <label>Notification body</label>
                                                <input type="text" name="body" class="form-control" id="notification-body" placeholder="Insert here a payload">
                                            </div>
                                            <div class="form-group">
                                                <label>Button label</label>
                                                <input type="text" name="button-label" class="form-control" id="notification-button-label" placeholder="Insert here a button label">
                                            </div>
                                            <div class="form-group">
                                                <label>Path Redirect</label>
                                                <input type="text" name="path-redirect" class="form-control" id="notification-path-redirect" placeholder="Insert here a path redirect">
                                            </div>
<!--                                            <div class="form-group">
                                                <label>Notification icon</label>
                                                <input type="file" name="file" id="fileuploadicon">
                                                <input type="button" value="Send Invoices" id="btnuploadicon"/>
                                                <input type="hidden" name="uploaddone" id="uploaddone" value="0">
                                            </div>-->
                                        </fieldset>
                                        <div>
                                            <div class="btn btn-primary" id="pushNotification">
                                                <i class="fa fa-save"></i>
                                                Submit
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>

                    </div>
                    <!--  Page content -->
                </div>
            </div>
        </div>

        <% include ../partials/footer %>


        <script src="/assets/bootstrap/js/bootstrap.min.js"></script>

        <script src="/assets/vendors/form-helpers/js/bootstrap-formhelpers.min.js"></script>

        <script src="/assets/vendors/select/bootstrap-select.min.js"></script>

        <script src="/assets/vendors/tags/js/bootstrap-tags.min.js"></script>

        <script src="/assets/vendors/mask/jquery.maskedinput.min.js"></script>

        <script src="/assets/vendors/moment/moment.min.js"></script>

        <script src="/assets/vendors/wizard/jquery.bootstrap.wizard.min.js"></script>
        <script src="/admin_tool/js/toolPushNotificationSafari.js"></script>

        <!-- bootstrap-datetimepicker -->
        <link href="/assets/vendors/bootstrap-datetimepicker/datetimepicker.css" rel="stylesheet">
        <script src="/assets/vendors/bootstrap-datetimepicker/bootstrap-datetimepicker.js"></script> 


        <link href="//cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/bootstrap3-editable/css/bootstrap-editable.css" rel="stylesheet"/>
        <script src="//cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/bootstrap3-editable/js/bootstrap-editable.min.js"></script>

        <script src="/assets/js/custom.js"></script>
        <script src="/assets/js/forms.js"></script>
    </body>
</html>